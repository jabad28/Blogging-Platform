<div class="row">
  <div class="large-12 columns ">
    <h1 class="fsahion-post-title"> Fashion Index Page</h1>
  </div>
</div>

<% if current_user != @user %>
  <% @posts.order(created_at: :desc).each do |post| %>
  User:<%= link_to post.user, post %><br>
  Title:<%= link_to post.title, post %><br>
  Body:<%= post.body %>
  <% end %>
<% end %>
